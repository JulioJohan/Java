
public class Arreglos {
	public static void main (String[] args) {
		int[] numeros = new int[3];
		int [] numero = {5,6,7,8};
		
		String[] heroes = new String[2]; 
		
		
		heroes[0]= "Saitama";
 		heroes[1] = "Garou";
		
		
		for(int i = 0; i<2; i++) {
			System.out.println(heroes[i]);
		}
		
		
		
		
		numeros[0] = 7;
		numeros[1] = 10;
		numeros[2] = 13;
		
						
		for(int i= 0; i<3; i++) {
			System.out.println(numeros[i]);
		}
		
		
	}
}
