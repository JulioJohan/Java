import java.util.Arrays;
import java.util.Scanner;

public class Ejercicio5 {
	public static void main(String [] args) {
		Scanner entrada  = new Scanner(System.in);
		int[] tabla1 = new int[10];
		int[] tabla2 =  new int [10];
		int[] concate = new int[20];
		
		System.out.println("Ingresa informacion de la primera tabla ...");
		for(int i = 0; i<10; i ++) {
			System.out.println("Ingresa un numero ");			
			tabla1[i] = entrada.nextInt();
		}
		System.out.println("Ingres la informacion de la segunda tabla ...");
		for(int i = 0; i<10; i++) {
			System.out.println("Ingresa un numero ");
			tabla2[i] = entrada.nextInt();
 		}
		
		System.out.println("Concatenacion de la tabla :" + Arrays.toString(concate));
		
		
		
		
		
	}
}
